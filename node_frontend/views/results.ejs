<!doctype html>
<html>
<head>
    <title>CCTL - Skeleton</title>
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/bootstrap/3.0.2/css/bootstrap.min.css"> 
    <link rel="stylesheet" href="//netdna.bootstrapcdn.com/font-awesome/4.0.3/css/font-awesome.min.css">
    <style>
        body        { padding-top:70px; }
    </style>
</head>
<body>
<div class="container">

    <div class="jumbotron text-center">
        <h1>Results</h1>
        <a href="/" class="btn btn-default btn-sm">Home</a>
        <a href="/survey" class="btn btn-default btn-sm">Survey</a>        
    </div>
    <table class="table">
        <tbody>
        <% var i = 0 %>
        <% while(i < concepts.length) { %>
            <tr>
            <% for(var j = 0; j < 3 && i < concepts.length; j++) { %>
            <td>
                <div class="well">
                    <h3> <%= concepts[i].name %></h3>

                        <p>
                            <strong><%= concepts[i++].difficulty %></strong>
                        </p>

                </div>
            </td>
            <%}%>
         <% } %>
        </tbody>
    </table>
   

</div>
</body>
</html>